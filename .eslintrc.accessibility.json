{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "plugin:jsx-a11y/recommended"
    ],
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "rules": {
      "jsx-a11y/anchor-ambiguous-text": "warn"
    },
    "ignorePatterns": [
      "out",
      "dist",
      "**/*.d.ts"
    ]
  }
  